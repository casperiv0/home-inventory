{
  "name": "home-inventory",
  "version": "0.1.0",
  "license": "MIT",
  "private": true,
  "scripts": {
    "format:quick": "pretty-quick --staged --pattern \"**/*.{js,jsx,ts,tsx,md,css,json}\"",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,md,css,json}\" --ignore-path .gitignore",
    "lint": "npx eslint . --ext .ts,.js,.tsx,.jsx",
    "dev": "npx prisma generate && npx prisma migrate dev && next dev",
    "build": "next build",
    "build-deploy": "npx prisma migrate deploy && npx prisma generate && next build",
    "prepare": "husky install",
    "test-tsc": "npx tsc --noEmit",
    "start": "npx prisma migrate deploy && npx prisma generate && next start"
  },
  "devDependencies": {
    "@casper124578/eslint-config": "^5.0.1",
    "@casper124578/eslint-config-next": "^5.0.0",
    "@casper124578/eslint-config-react": "^5.0.0",
    "@types/node": "^18.7.23",
    "@types/react": "^18.0.21",
    "@types/react-dom": "^18.0.6",
    "autoprefixer": "^10.4.12",
    "eslint": "^8.24.0",
    "husky": "^8.0.1",
    "postcss": "^8.4.17",
    "prettier": "^2.7.1",
    "pretty-quick": "^3.1.3",
    "prisma": "^4.4.0",
    "tailwindcss": "^3.1.8",
    "typescript": "^4.8.4"
  },
  "dependencies": {
    "@casper124578/useful": "^2.0.4",
    "@hookform/resolvers": "^2.9.8",
    "@prisma/client": "^4.4.0",
    "@radix-ui/react-dialog": "^1.0.0",
    "@radix-ui/react-dropdown-menu": "^1.0.0",
    "@react-aria/label": "^3.4.1",
    "@tanstack/react-query": "^4.8.0",
    "@tanstack/react-table": "^8.5.13",
    "@trpc/client": "^10.0.0-proxy-beta.0",
    "@trpc/next": "^10.0.0-proxy-beta.0",
    "@trpc/react": "^10.0.0-proxy-beta.10",
    "@trpc/server": "^10.0.0-proxy-beta.0",
    "next": "^12.3.1",
    "next-auth": "^4.12.2",
    "react": "^18.2.0",
    "react-bootstrap-icons": "^1.9.1",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.36.1",
    "superjson": "^1.10.0",
    "tailwind-merge": "^1.6.1",
    "zod": "^3.19.1"
  }
}
