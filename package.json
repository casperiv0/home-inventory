{
  "name": "home-inventory",
  "version": "0.1.0",
  "license": "MIT",
  "private": true,
  "scripts": {
    "format:quick": "pretty-quick --staged --pattern \"**/*.{js,jsx,ts,tsx,md,css,json}\"",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,md,css,json}\" --ignore-path .gitignore",
    "lint": "npx eslint . --ext .ts,.js,.tsx,.jsx",
    "dev": "npx prisma generate && npx prisma migrate dev && next dev",
    "build": "next build",
    "build-deploy": "npx prisma migrate deploy && npx prisma generate && next build",
    "prepare": "husky install",
    "test-tsc": "npx tsc --noEmit",
    "start": "npx prisma migrate deploy && npx prisma generate && next start"
  },
  "devDependencies": {
    "@casper124578/eslint-config": "^5.0.1",
    "@casper124578/eslint-config-next": "^6.0.0-alpha.0",
    "@casper124578/eslint-config-react": "^5.0.0",
    "@types/node": "^18.11.18",
    "@types/react": "^18.0.26",
    "@types/react-dom": "^18.0.10",
    "autoprefixer": "^10.4.13",
    "eslint": "^8.31.0",
    "husky": "^8.0.2",
    "postcss": "^8.4.20",
    "prettier": "^2.8.1",
    "pretty-quick": "^3.1.3",
    "prisma": "^4.13.0",
    "tailwindcss": "^3.2.4",
    "typescript": "^4.9.4"
  },
  "dependencies": {
    "@casper124578/useful": "^2.0.6",
    "@hookform/resolvers": "^2.9.10",
    "@prisma/client": "^4.8.0",
    "@radix-ui/react-dialog": "^1.0.2",
    "@radix-ui/react-dropdown-menu": "^1.0.0",
    "@react-aria/label": "^3.4.4",
    "@tanstack/react-query": "^4.20.4",
    "@tanstack/react-table": "^8.7.4",
    "@trpc/client": "^10.7.0",
    "@trpc/next": "^10.7.0",
    "@trpc/react-query": "^10.7.0",
    "@trpc/server": "^10.7.0",
    "next": "^13.1.1",
    "next-auth": "^4.18.7",
    "react": "^18.2.0",
    "react-bootstrap-icons": "^1.10.2",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.41.3",
    "superjson": "^1.12.1",
    "tailwind-merge": "^1.8.1",
    "zod": "^3.20.2"
  }
}
