{
  "name": "home-inventory",
  "version": "0.1.0",
  "license": "MIT",
  "private": true,
  "scripts": {
    "format:quick": "pretty-quick --staged --pattern \"**/*.{js,jsx,ts,tsx,md,css,json}\"",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,md,css,json}\" --ignore-path .gitignore",
    "lint": "npx eslint . --ext .ts,.js,.tsx,.jsx",
    "dev": "npx prisma generate && npx prisma migrate dev && next dev",
    "build": "next build",
    "build-deploy": "npx prisma migrate deploy && npx prisma generate && next build",
    "prepare": "husky install",
    "test-tsc": "npx tsc --noEmit",
    "start": "npx prisma migrate deploy && npx prisma generate && next start"
  },
  "devDependencies": {
    "@casper124578/eslint-config": "^4.4.0",
    "@casper124578/eslint-config-next": "^4.4.0",
    "@casper124578/eslint-config-react": "^4.4.0",
    "@types/node": "^18.7.14",
    "@types/react": "^18.0.18",
    "@types/react-dom": "^18.0.6",
    "autoprefixer": "^10.4.8",
    "eslint": "^8.23.0",
    "husky": "^8.0.1",
    "postcss": "^8.4.16",
    "prettier": "^2.7.1",
    "pretty-quick": "^3.1.3",
    "prisma": "^4.2.1",
    "tailwindcss": "^3.1.8",
    "typescript": "^4.8.2"
  },
  "dependencies": {
    "@casper124578/useful": "^2.0.3",
    "@hookform/resolvers": "^2.9.7",
    "@prisma/client": "^4.2.1",
    "@radix-ui/react-dialog": "^1.0.0",
    "@radix-ui/react-dropdown-menu": "^1.0.0",
    "@react-aria/label": "^3.4.1",
    "@tanstack/react-table": "^8.5.11",
    "@trpc/client": "^9.27.2",
    "@trpc/next": "^9.27.2",
    "@trpc/react": "^9.27.2",
    "@trpc/server": "^9.27.2",
    "next": "^12.2.5",
    "next-auth": "^4.10.3",
    "react": "^18.2.0",
    "react-bootstrap-icons": "^1.8.4",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.34.2",
    "react-query": "^3.39.2",
    "superjson": "^1.9.1",
    "tailwind-merge": "^1.6.0",
    "zod": "^3.18.0"
  }
}
